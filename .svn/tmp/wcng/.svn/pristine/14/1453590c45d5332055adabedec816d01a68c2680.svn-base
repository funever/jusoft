<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:minWidth="900dp"
    android:layout_height="48dp"
    android:orientation="horizontal" 
    android:gravity="center_vertical"
    android:id="@+id/baobiao_jylc_title"
    >
    <TextView 
        android:id="@+id/prolist_spqm"
        android:layout_width="0px"
        android:layout_height="48dp"
        android:layout_weight="2"
        android:text="商品名称"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:gravity="center"
        android:background="@color/select_pro_c1"
        />
	<TextView 
        android:id="@+id/pro_danwei"
        android:layout_width="0px"
        android:layout_height="48dp"
        android:layout_weight="1.5"
        android:text="单位"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:gravity="center"
        android:background="@color/select_pro_c2"
        />
	<TextView 
        android:id="@+id/pro_shuliang"
        android:layout_width="0px"
        android:layout_height="48dp"
        android:layout_weight="1"
        android:text="数量"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:gravity="center"
        android:background="@color/select_pro_c1"
        />
	<TextView 
        android:id="@+id/pro_danjia"
        android:layout_width="0px"
        android:layout_height="48dp"
        android:layout_weight="1"
        android:text="单价"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:gravity="center"
        android:background="@color/select_pro_c2"
        />
	<TextView 
        android:id="@+id/pro_je"
        android:layout_width="0px"
        android:layout_height="48dp"
        android:layout_weight="1"
        android:text="金额"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:gravity="center"
        android:background="@color/select_pro_c1"
        />
	<TextView 
        android:id="@+id/pro_fzsl"
        android:layout_width="0px"
        android:layout_height="48dp"
        android:layout_weight="1.5"
        android:text="辅助数量"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:gravity="center"
        android:background="@color/select_pro_c2"
        />
	<TextView 
        android:id="@+id/guige"
        android:layout_width="0px"
        android:layout_height="48dp"
        android:layout_weight="1"
        android:text="规格"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:gravity="center"
        android:background="@color/select_pro_c1"
        />
	<TextView 
        android:id="@+id/xinghao"
        android:layout_width="0px"
        android:layout_height="48dp"
        android:layout_weight="1.2"
        android:text="型号"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:gravity="center"
        android:background="@color/select_pro_c2"
        />
	<TextView 
        android:id="@+id/zp"
        android:layout_width="0px"
        android:layout_height="48dp"
        android:layout_weight="1"
        android:text="赠品"
        android:textColor="@android:color/black"
        android:textSize="16dp"
        android:gravity="center"
        android:background="@color/select_pro_c1"
        />
	<ImageView 
   	    android:layout_width="6dip"
      	android:layout_height="12dip"
       	android:layout_marginRight="15dip"
       	android:src="@drawable/right_arrow"
       	android:id="@+id/right_arrow"
       	android:visibility="invisible"
   	    />
</LinearLayout>
