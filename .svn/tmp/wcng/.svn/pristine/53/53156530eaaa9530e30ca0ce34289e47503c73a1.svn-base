<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:id="@+id/operate_in" 
    android:background="@drawable/main_bg"
    >
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/titlebar_background" >
        <TextView
            android:id="@+id/navi_title"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:textSize="16dp"
            android:text="添加订单"
            android:textColor="#ffffff"
             />
        <Button 
            android:layout_width="55dp"
            android:layout_height="38dp"
            android:id="@+id/nv_return"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:text="返回"
            android:textColor="#ffffff"
            android:paddingLeft="5dp"
            android:background="@drawable/titlebar_back_btn_background"
            />
        <Button 
            android:layout_width="55dp"
            android:layout_height="35dp"
            android:id="@+id/nv_save"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="保存"
            android:textColor="#ffffff"
            android:layout_marginRight="5dp"
            android:background="@drawable/titlebar_help_btn_background"
            />
    </RelativeLayout>
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:orientation="vertical"
        android:background="@drawable/all_round"
        >
        <RelativeLayout 
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            android:id="@+id/ldrq_action"
            >
            <TextView 
	        android:text="录单日期:"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        android:textSize="16dp"
	        />
	    	<TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:id="@+id/ldrq_edit"
	        android:gravity="center_vertical"
	        android:layout_marginLeft="100dp"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        />
	    	<ImageView 
            	android:layout_width="6dip"
            	android:layout_height="12dip"
            	android:layout_alignParentRight="true"
           		android:layout_marginRight="15dip"
            	android:layout_centerVertical="true"
            	android:src="@drawable/home_index_arrow"/>
        </RelativeLayout>
        <TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
        <RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            >
            <TextView 
	        android:text="单据编号:"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        android:textSize="16dp"
	        />
	    	<EditText 
	        android:layout_width="match_parent"
	        android:layout_height="fill_parent"
	        android:id="@+id/ldbh_edit"
	        android:textSize="17dp"
	        android:singleLine="true"
	        android:gravity="center_vertical"
	        android:textColor="#0000ff"
	        android:layout_marginLeft="100dp"
	        android:layout_marginRight="5dp"
	        android:layout_marginTop="3dp"
	        android:layout_marginBottom="3dp"
	        android:paddingLeft="3dp"
	        android:background="@drawable/edit_style"
	        />
        </RelativeLayout>
        <TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
        <RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            android:id="@+id/wldw_action"
            >
	    	 <TextView 
	        android:text="付款单位:"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        android:textSize="16dp"
	        android:id="@+id/dw_title"
	        />
	    	<EditText 
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:id="@+id/wldw_edit"
	        android:layout_marginLeft="100dp"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        android:background="@drawable/edit_style"
	       android:layout_marginRight="40dp"
	        android:layout_centerVertical="true"
	        />
	    	<ImageView 
	    	    android:layout_width="30dip"
            	android:layout_height="30dip"
            	android:layout_alignParentRight="true"
            	android:layout_centerVertical="true"
            	android:src="@drawable/search"
	    	    android:id="@+id/dw_add"
	    	    android:layout_marginRight="7dp"
	    	    />
	    </RelativeLayout>
	    <TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
	    <RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            android:id="@+id/jsr_action"
            >
	    	<TextView 
	        android:text="经手人:"
	        android:textSize="16dp"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        />
	   		<EditText 
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:id="@+id/jsr_edit"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        android:layout_marginLeft="100dp"
	        android:background="@drawable/edit_style"
	        android:layout_marginRight="40dp"
	        android:layout_centerVertical="true"
	        />
	   		<ImageView 
	    	    android:layout_width="30dp"
            	android:layout_height="30dp"
            	android:layout_alignParentRight="true"
            	android:layout_centerVertical="true"
            	android:src="@drawable/search"
	    	    android:id="@+id/jsr_add"
	    	    android:layout_marginRight="7dp"
	    	    />
	    </RelativeLayout>
	   <TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            android:visibility="gone"
            />
	    <RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            android:id="@+id/bm_action"
            android:visibility="gone"
            >
	    <TextView 
	        android:text="部门:"
	        android:textSize="16dp"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        />
	    <EditText 
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:id="@+id/bm_edit"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        android:layout_marginLeft="100dp"
	        android:background="@drawable/edit_style"
	        android:layout_marginRight="5dp"
	        android:layout_centerVertical="true"
	        />
	    <ImageView 
	    	    android:layout_width="30dp"
            	android:layout_height="30dp"
            	android:layout_alignParentRight="true"
            	android:layout_centerVertical="true"
            	android:src="@drawable/search"
	    	    android:id="@+id/bm_add"
	    	    android:layout_marginRight="7dp"
	    	    />
	</RelativeLayout>
	<TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
	<RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="收款金额:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:id="@+id/je_title"
                    android:textColor="#000000" />
                <EditText 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/money_edit"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:textColor="#0000ff"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="40dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:numeric="decimal"
			        />
                <TextView 
	                android:layout_width="30dp"
                    android:layout_height="30dp"
	                android:layout_alignParentRight="true"
	                android:layout_centerVertical="true"
	                android:textColor="#0000bb"
	                android:textSize="17dp"
	                android:layout_marginRight="10dp"
	                android:id="@+id/je_more"
	                android:background="@drawable/search"
                />
            </RelativeLayout>
            <TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" 
                android:id="@+id/zh_action"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="收款账户:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" 
                    android:id="@+id/zh_title"
                    />
                <EditText
                    android:id="@+id/zh"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginLeft="100dp"
                    android:layout_centerVertical="true"
                    android:singleLine="true"
                    android:textColor="#0000ff"
				    android:textSize="17dp"
				    android:paddingLeft="5dp"
                    android:background="@drawable/edit_style"
                   android:layout_marginRight="40dp"
                    />
                <ImageView
                    android:layout_marginRight="7dp"
                    android:id="@+id/zh_add"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/search" />
            </RelativeLayout>
	<TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
	<RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            >
	    <TextView 
	        android:text="摘要:"
	        android:textSize="16dp"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        />
	    <EditText 
	        android:layout_width="match_parent"
	        android:layout_height="fill_parent"
	        android:id="@+id/zy_edit"
	        android:textSize="17dp"
	        android:singleLine="true"
	        android:gravity="center_vertical"
	        android:textColor="#0000ff"
	        android:layout_marginLeft="100dp"
	        android:layout_marginRight="5dp"
	        android:layout_marginTop="3dp"
	        android:layout_marginBottom="3dp"
	        android:paddingLeft="3dp"
	        android:background="@drawable/edit_style"
	        />
	</RelativeLayout>
    </LinearLayout>
</LinearLayout>