<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@android:color/white"
    >
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/titlebar_background" >
        <TextView
            android:id="@+id/navi_title"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:textSize="18dp"
            android:text="公司信息"
            android:textColor="#ffffff"
             />
        <Button 
            android:layout_width="55dp"
            android:layout_height="38dp"
            android:id="@+id/nv_return"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:text="返回"
            android:textColor="#ffffff"
            android:paddingLeft="5dp"
            android:background="@drawable/titlebar_back_btn_background"
            />
        <Button
            android:layout_width="55dp"
            android:layout_height="38dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:background="@drawable/titlebar_help_btn_background"
            android:text="编辑"
            android:id="@+id/edit"
            android:textColor="#ffffff" />
    </RelativeLayout>
	<LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="15dp"
        android:orientation="vertical"
        android:background="@drawable/all_round"
        >
       <RelativeLayout
        android:id="@+id/ck_action"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:gravity="center_vertical"
        android:paddingLeft="5dp" >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:gravity="center_vertical"
            android:text="公司名称:"
            android:textColor="#000000"
            android:id="@+id/ck_title"
            android:layout_marginLeft="10dp"
            android:textSize="16dp" />
        <EditText
            android:id="@+id/cp_name"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@id/ck_title"
            android:background="@drawable/edit_style"
            android:textColor="#0000ff"
            android:layout_marginRight="5dp"
            android:enabled="false"
            android:paddingLeft="5dp"
            android:textSize="17dp" />
    </RelativeLayout>
        <TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
        <RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            >
	    	 <TextView 
	        android:text="联系电话:"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        android:textSize="16dp"
	        android:layout_marginLeft="10dp"
	        android:id="@+id/dw_title"
	        />
	    	<EditText 
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:id="@+id/cp_tel"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        android:background="@drawable/edit_style"
	        android:layout_marginRight="5dp"
	        android:layout_centerVertical="true"
	        android:layout_toRightOf="@id/dw_title"
	        android:enabled="false"
	        android:paddingLeft="5dp"
	        />
	    </RelativeLayout>
	    <TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
        <RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="5dp"
            android:gravity="center_vertical"
            android:layout_marginBottom="15dp"
            >
	    	 <TextView 
	        android:text="公司地址:"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_horizontal"
	        android:textColor="#000000"
	        android:textSize="16dp"
	        android:layout_marginLeft="10dp"
	        android:id="@+id/dw_title"
	        android:paddingTop="10dp"
	        />
	    	<EditText 
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        android:id="@+id/cp_address"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        android:background="@drawable/edit_style"
	        android:layout_marginRight="5dp"
	        android:layout_centerVertical="true"
	        android:layout_toRightOf="@id/dw_title"
	        android:enabled="false"
	        android:gravity="left|top"
	        android:paddingLeft="5dp"
	        android:paddingTop="10dp"
	        />
	    </RelativeLayout>
        </LinearLayout>
</LinearLayout>
