<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@drawable/main_bg"
    >
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/titlebar_background" >
        <TextView
            android:id="@+id/navi_title"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:textSize="16dp"
            android:text="选择商品"
            android:textColor="#ffffff"
             />
        <Button 
            android:layout_width="55dp"
            android:layout_height="38dp"
            android:id="@+id/nv_return"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:text="返回"
            android:textColor="#ffffff"
            android:paddingLeft="5dp"
            android:background="@drawable/titlebar_back_btn_background"
            />
        <Button 
            android:layout_width="55dp"
            android:layout_height="38dp"
            android:id="@+id/nv_save"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="确定"
            android:textColor="#ffffff"
            android:layout_marginRight="5dp"
            android:background="@drawable/titlebar_help_btn_background"
            />
    </RelativeLayout>
    <ScrollView 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        >
        <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:orientation="vertical" 
            android:background="@drawable/all_round">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" 
                android:id="@+id/choose_pro"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="选择商品:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView
                    android:id="@+id/pro_name"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="100dp"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:textColor="@android:color/black"
                    android:textSize="15dp" />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:orientation="vertical" 
            android:background="@drawable/all_round">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="单位关系:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_urate"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:text="1"
			        />
            </RelativeLayout>
            <!-- <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="单位:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <com.jetsoft.view.CustomSinnper 
				    android:id="@+id/pro_danwei"
				    android:layout_width="match_parent"
				    android:layout_height="fill_parent"
				    android:text=""
				    android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
				    >	   
				</com.jetsoft.view.CustomSinnper>
            </RelativeLayout>  -->
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="数量:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <EditText 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_num"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:textColor="#0000ff"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:text="1"
			        android:numeric="decimal"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="单价:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <EditText 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_count"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:textColor="#0000ff"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:text="1"
			        android:numeric="decimal"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="折扣:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <EditText 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_zhekou"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:textColor="#0000ff"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:text="1"
			        android:numeric="decimal"
			        />
            </RelativeLayout>
             <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            android:id="@+id/line_zhekou"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="税率:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <EditText 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_shuilv"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:textColor="#0000ff"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:text="0"
			        android:numeric="decimal"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            android:id="@+id/line_shuilv"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="明细备注:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <EditText 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_memo"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:textColor="#0000ff"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            android:id="@+id/line_shuilv"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" 
                android:id="@+id/zengping_layout"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="是否赠品:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <CheckBox 
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:id="@+id/pro_zengpin"
			        android:layout_alignParentRight="true"
			        android:layout_marginRight="5dp"
			        />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:orientation="horizontal" 
            >
            <Button 
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:text="图片"
                android:background="@drawable/transparent"
                android:layout_weight="1"
                android:id="@+id/pro_img"
                />
            <Button 
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:text="替代品"
                android:background="@drawable/transparent"
                android:layout_weight="1"
                android:id="@+id/pro_replace"
                android:layout_marginLeft="1dp"
                />
            <Button 
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:text="库存分布"
                android:background="@drawable/transparent"
                android:layout_weight="1"
                android:id="@+id/pro_kucun"
                android:layout_marginLeft="1dp"
                />
            <Button 
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:text="预设"
                android:background="@drawable/transparent"
                android:layout_weight="1"
                android:id="@+id/pro_price"
                android:layout_marginLeft="1dp"
                />
            <Button 
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:text="备注"
                android:background="@drawable/transparent"
                android:layout_weight="1"
                android:id="@+id/pro_beizhu"
                android:layout_marginLeft="1dp"
                />
        </LinearLayout>
        <LinearLayout 
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:orientation="horizontal"
            >
            <Button
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="@drawable/button_del_round"
            android:id="@+id/pro_delete"
            android:text="删除商品"
            android:textSize="20dp"
            />
            <Button
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="@drawable/button_del_round"
            android:id="@+id/pro_copy"
            android:text="复制商品"
            android:textSize="20dp"
            android:layout_marginLeft="20dp"
            />
        </LinearLayout>
    </LinearLayout>
    </ScrollView>
</LinearLayout>