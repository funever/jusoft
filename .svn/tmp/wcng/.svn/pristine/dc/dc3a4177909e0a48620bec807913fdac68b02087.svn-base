<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:id="@+id/operate_in" 
    android:background="@drawable/main_bg"
    >
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/titlebar_background" >
        <TextView
            android:id="@+id/navi_title"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:text="商品进货汇总表"
            android:textColor="#ffffff"
            android:textSize="16dp" />
        <Button 
            android:layout_width="55dp"
            android:layout_height="38dp"
            android:id="@+id/nv_return"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:text="返回"
            android:textColor="#ffffff"
            android:paddingLeft="7dp"
            android:background="@drawable/titlebar_back_btn_background"
            />
    </RelativeLayout>
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:orientation="vertical"
        android:background="@drawable/all_round"
        >
        <RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            >
	    	 <TextView 
	        android:text="往来单位:"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        android:textSize="16dp"
	        />
	    	<EditText 
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:id="@+id/dw_edit"
	        android:layout_marginLeft="100dp"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        android:background="@drawable/edit_style"
	        android:layout_marginRight="40dp"
	        android:layout_centerVertical="true"
	        />
	    	<ImageView 
	    	    android:layout_width="30dip"
            	android:layout_height="30dip"
            	android:layout_alignParentRight="true"
            	android:layout_centerVertical="true"
            	android:src="@drawable/search"
	    	    android:id="@+id/dw_action"
	    	    android:layout_marginRight="7dp"
	    	    />
	    </RelativeLayout>
	    <TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
	    <RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            >
	    	<TextView 
	        android:text="职员:"
	        android:textSize="16dp"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        />
	   		<EditText 
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:id="@+id/jsr_edit"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        android:layout_marginLeft="100dp"
	        android:background="@drawable/edit_style"
	        android:layout_marginRight="40dp"
	        android:layout_centerVertical="true"
	        />
	   		<ImageView 
	    	    android:layout_width="30dp"
            	android:layout_height="30dp"
            	android:layout_alignParentRight="true"
            	android:layout_centerVertical="true"
            	android:src="@drawable/search"
	    	    android:id="@+id/jsr_action"
	    	    android:layout_marginRight="7dp"
	    	    />
	    </RelativeLayout>
	   <TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            android:visibility="gone"
            />
	    <RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            android:visibility="gone"
            >
	    <TextView 
	        android:text="部门:"
	        android:textSize="16dp"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        />
	    <EditText 
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:id="@+id/bm_edit"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        android:layout_marginLeft="100dp"
	        android:background="@drawable/edit_style"
	        android:layout_marginRight="5dp"
	        android:layout_centerVertical="true"
	        />
	    <ImageView 
	    	    android:layout_width="30dp"
            	android:layout_height="30dp"
            	android:layout_alignParentRight="true"
            	android:layout_centerVertical="true"
            	android:src="@drawable/search"
	    	    android:id="@+id/bm_action"
	    	    android:layout_marginRight="7dp"
	    	    />
	</RelativeLayout>
	<TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
	<RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            >
	    <TextView 
	        android:text="仓库:"
	        android:textSize="16dp"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        />
	    <EditText 
	        android:layout_width="match_parent"
	        android:layout_height="40dp"
	        android:id="@+id/ck_edit"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        android:layout_marginLeft="100dp"
	        android:background="@drawable/edit_style"
	        android:layout_marginRight="40dp"
	        android:layout_centerVertical="true"
	        />
	   <ImageView 
	    	    android:layout_width="30dp"
            	android:layout_height="30dp"
            	android:layout_alignParentRight="true"
            	android:layout_centerVertical="true"
            	android:src="@drawable/search"
	    	    android:id="@+id/ck_action"
	    	    android:layout_marginRight="7dp"
	    	    />
	</RelativeLayout>
	<TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@android:color/darker_gray"
            />
        <RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            >
	    <TextView 
	        android:text="是否显示为0行:"
	        android:textSize="16dp"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        />
	    <CheckBox 
	    	    android:layout_width="wrap_content"
            	android:layout_height="wrap_content"
            	android:layout_alignParentRight="true"
           		android:layout_marginRight="15dip"
            	android:layout_centerVertical="true"
	    	    android:id="@+id/show_zero"
	    	    android:checked="true"
	    	    />
		</RelativeLayout>
	<TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
	<RelativeLayout 
           	android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            android:id="@+id/start_date"
            >
	    <TextView 
	        android:text="开始时间:"
	        android:textSize="16dp"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        />
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:id="@+id/start_date_text"
	        android:gravity="center_vertical"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        android:layout_marginLeft="100dp"
	        />
	    <ImageView 
	    	    android:layout_width="6dip"
            	android:layout_height="12dip"
            	android:layout_alignParentRight="true"
           		android:layout_marginRight="15dip"
            	android:layout_centerVertical="true"
            	android:src="@drawable/home_index_arrow"
	    	    android:id="@+id/dhrq_add"
	    	    />
	</RelativeLayout>
	<TextView 
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="#cccccc"
            />
	<RelativeLayout 
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            android:id="@+id/end_date"
            >
            <TextView 
	        android:text="结束时间:"
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:gravity="center_vertical"
	        android:textColor="#000000"
	        android:textSize="16dp"
	        />
	    	<TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="fill_parent"
	        android:id="@+id/end_date_text"
	        android:gravity="center_vertical"
	        android:layout_marginLeft="100dp"
	        android:textColor="#0000ff"
	        android:textSize="17dp"
	        />
	    	<ImageView 
            	android:layout_width="6dip"
            	android:layout_height="12dip"
            	android:layout_alignParentRight="true"
           		android:layout_marginRight="15dip"
            	android:layout_centerVertical="true"
            	android:src="@drawable/home_index_arrow"/>
        </RelativeLayout>
    </LinearLayout>
    <Button 
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:orientation="vertical"
        android:background="@drawable/all_round"
        android:text="查  询"
        android:textSize="17dp"
        android:id="@+id/search"
        />
</LinearLayout>