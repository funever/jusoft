<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="@drawable/main_bg"
    >
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:background="@drawable/titlebar_background" >
        <TextView
            android:id="@+id/navi_title"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:textSize="16dp"
            android:text="选择商品"
            android:textColor="#ffffff"
             />
        <Button 
            android:layout_width="55dp"
            android:layout_height="38dp"
            android:id="@+id/nv_return"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:text="返回"
            android:textColor="#ffffff"
            android:paddingLeft="5dp"
            android:background="@drawable/titlebar_back_btn_background"
            />
        <Button 
            android:layout_width="55dp"
            android:layout_height="38dp"
            android:id="@+id/nv_save"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="确定"
            android:textColor="#ffffff"
            android:layout_marginRight="5dp"
            android:background="@drawable/titlebar_help_btn_background"
            />
    </RelativeLayout>
	<LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:orientation="vertical" 
            android:background="@drawable/all_round">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" 
                android:id="@+id/choose_pro"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="商品全名:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView
                    android:id="@+id/pro_name"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="100dp"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:textColor="@android:color/black"
                    android:textSize="15dp" />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="规格:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_standard"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:text="1"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="型号:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_type"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:text="1"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="产地:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_area"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:text="1"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="基本单位:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_unit1"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:text="1"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="辅助单位:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_unit2"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:text="1"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="单位关系:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_urate"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:text="1"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="当前库存:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_num"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:text="1"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="40dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="虚拟库存:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_voidnum"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:text="1"
			        />
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:orientation="horizontal" 
            android:layout_gravity="center_vertical"
            >
            <Button 
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:text="图片"
                android:background="@drawable/transparent"
                android:layout_weight="1"
                android:id="@+id/pro_img"
                 android:layout_marginLeft="1dp"
                />
            <Button 
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:text="替代品"
                android:background="@drawable/transparent"
                android:layout_weight="1"
                android:id="@+id/pro_replace"
                 android:layout_marginLeft="1dp"
                />
            <Button 
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:text="库存分布"
                android:background="@drawable/transparent"
                android:layout_weight="1"
                android:id="@+id/pro_kucun"
                 android:layout_marginLeft="1dp"
                />
            <Button 
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:text="预设"
                android:background="@drawable/transparent"
                android:layout_weight="1"
                android:id="@+id/pro_price"
                 android:layout_marginLeft="1dp"
                />
            <Button 
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:text="备注"
                android:background="@drawable/transparent"
                android:layout_weight="1"
                android:id="@+id/pro_beizhu"
                android:layout_marginLeft="1dp"
                />
        </LinearLayout>
</LinearLayout>