<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    >
    <ScrollView 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        >
        <LinearLayout 
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:orientation="vertical" 
            android:background="@drawable/all_round">
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="商品全名:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_name"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:text="1"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="库存数量:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_kucunnum"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:text="1"
			        android:editable="false"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            android:visibility="gone"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" 
                android:visibility="gone"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="成本单价:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_price"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:text="1"
			        android:editable="false"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            android:visibility="gone"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" 
                android:visibility="gone"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="成本金额:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_amount"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:text="1"
			        android:numeric="decimal"
			        android:editable="false"
			        />
            </RelativeLayout>
             <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            android:id="@+id/line_zhekou"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="规格:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_guige"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:text="0"
					android:editable="false"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            android:id="@+id/line_shuilv"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="型号:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_xinghao"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:editable="false"
			        />
            </RelativeLayout>
            <TextView 
	            android:layout_width="fill_parent"
	            android:layout_height="1px"
	            android:background="#cccccc"
	            android:id="@+id/line_shuilv"
	            />
            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="45dp"
                android:clickable="true"
                android:focusable="true"
                android:gravity="center_vertical"
                android:paddingLeft="10dp" >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="产地:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:textColor="#000000" />
                <TextView 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_changdi"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:editable="false"
			        />
            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/all_round"
            >
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:text="盘点数量:"
                    android:textSize="16dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:textColor="#000000" />
                <EditText 
			        android:layout_width="match_parent"
			        android:layout_height="fill_parent"
			        android:id="@+id/pro_pandiannum"
			        android:textSize="17dp"
			        android:singleLine="true"
			        android:gravity="center_vertical"
			        android:textColor="#0000ff"
			        android:layout_marginLeft="100dp"
			        android:layout_marginRight="5dp"
			        android:layout_marginTop="3dp"
			        android:layout_marginBottom="3dp"
			        android:paddingLeft="3dp"
			        android:background="@drawable/edit_style"
			        android:numeric="integer"
			        />
        </RelativeLayout>
        <Button
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="15dp"
            android:orientation="vertical" 
            android:background="@drawable/button_del_round"
            android:id="@+id/pro_delete"
            android:text="删除商品"
            android:textSize="20dp"
            >
        </Button>
        </LinearLayout>
    </ScrollView>
</LinearLayout>