            />
        <RelativeLayout 
            android:layout_width="fill_parent"
            android:layout_height="45dp"
            android:clickable="true"
            android:focusable="true"
            android:paddingLeft="10dp"
            android:gravity="center_vertical"
            android:id="@+id/reg_action"
