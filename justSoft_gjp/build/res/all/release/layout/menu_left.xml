<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="15dp"
    android:paddingLeft="10dp"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="帐套和用户"
        android:textSize="15dp"
        style="@style/Category"
        />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="帐套"
        android:textColor="@android:color/black"
        android:textSize="17dp"
        />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#d2d2d2"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="[未选择帐套]"
        android:textSize="17dp"
        android:textColor="#0000aa"
        />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#d2d2d2"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="用户"
        android:textColor="@android:color/black"
        android:textSize="17dp"
        />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#d2d2d2"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        />
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="[未登录]"
        android:textSize="17dp"
        android:textColor="#0000aa"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="其他设置"
        android:textSize="15dp"
        style="@style/Category"
        />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:id="@+id/scanner"
            >
            <TextView
                android:text="扫描器设置"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textSize="16dp"
                android:textColor="@android:color/black"
                />
            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_arrow"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                />
        </RelativeLayout>
        <TextView
            android:layout_width="match_parent"
            android:background="#d3d3d3"
            android:layout_height="1dp"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:id="@+id/painter"
            >
            <TextView
                android:text="打印机设置"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textSize="16dp"
                android:textColor="@android:color/black"
                android:id="@+id/painter_title"
                />
            <TextView
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textColor="#0000ff"
                android:layout_marginRight="25dp"
                android:textSize="15dp"
                android:layout_alignParentRight="true"
                android:id="@+id/show_ip_port"
                />
            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_arrow"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                />
        </RelativeLayout>
        <TextView
            android:layout_width="match_parent"
            android:background="#d3d3d3"
            android:layout_height="1dp"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:id="@+id/menu_set"
            >
            <TextView
                android:text="菜单设置"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textColor="@android:color/black"
                android:textSize="16dp"
                android:id="@+id/menu_set_title"
                />
            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_arrow"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                />
        </RelativeLayout>
        <TextView
            android:layout_width="match_parent"
            android:background="#d3d3d3"
            android:layout_height="1dp"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:id="@+id/company_info"
            >
            <TextView
                android:text="公司信息"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:textSize="16dp"
                android:textColor="@android:color/black"
                />
            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_centerVertical="true"
                android:src="@drawable/right_arrow"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout><!-- From: file:/E:/android_studio_workspace/jusoft/justSoft_gjp/src/main/res/layout/menu_left.xml -->